<div class="row text-center">
  <div class="col-sm-4"><h4>FREEUSER COUNT: <span class="label label-info"><%= @references_count || 0 %></span></h4></div>
  <div class="col-sm-4"><h4>LIFEMEMBERSHIP</h4></div>  
</div>

<div class="row">
  <div class="col-sm-4 profile">
    <ul class="list-group">
      <% if current_user.avatar.present? %>
      <%= image_tag current_user.avatar_url, class: "img-responsive" %>
      <% elsif current_user.gender %>
      <%= image_tag "profile-2.jpg", class: "img-responsive" %>
      <% else %>
      <%= image_tag "profile-1.jpg", class: "img-responsive" %>
      <%end %>

      <li class="list-group-item">
        <div class="row">        
          <div class="col-xs-12"><%= current_user.name %></div>
        </div>
      </li>
    </ul>
  </div>
  <div class="col-sm-4 text-center">
    <%= image_tag "logo.jpg", size: '150x150'%>    

  </div>
  <div class="col-sm-4 text-left">

    <%= image_tag "customer-page.jpg", size: '200x200', class: "rotate-image"%>    


  </div>
</div>
<br>
